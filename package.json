    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
